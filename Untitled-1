// Map - buhat ug bagong array variable na lahi ang purma pero based xa sa X.

const x = [{
  name : "Manga",
  quantity : 3
},
{
  name : "Bayabas",
  quantity: 4
},
{
  name : "Melon",
  quantity: 10
}]

