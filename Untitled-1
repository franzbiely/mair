// Exmple sa Mapping : 
const y = x.map((currentX) => {
  return currentX.quantity
})
console.log({y})

// Filter - mag return ug bagong array na based sa X pero based sa condition.

const z = x.filter((currentX) => {
  return (currentX.name === "Manga")
}).map((currentX) => {
  return currentX.quantity
})


